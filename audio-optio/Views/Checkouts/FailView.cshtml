@model List<String>

<div class="jumbotron alert alert-danger">
    <h2>Oops!</h2>

    Something went wrong...
</div>


@if(Model.Count > 0)
{ 
    <div class="alert alert-danger" role="alert">
        <a class="close" data-dismiss="alert">×</a>
        @foreach(String error in Model)
        {
            <p>@error</p>
        }
    </div>
}